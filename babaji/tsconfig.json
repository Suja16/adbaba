{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "@typings/gql": ["types/gql/index"]
    },
    "noEmit": true
  },
  "include": ["src", "types/gql"],
  "exclude": ["node_modules"]
}
